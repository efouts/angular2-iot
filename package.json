{
  "name": "angular2-iot",
  "version": "0.3.0",
  "description": "IoT support for Angular 2",
  "scripts": {
    "build": "rimraf dist compiled && ngc",
    "test": "tslint src/**/*.ts && npm run build && npm run example:build",
    "example:build": "rimraf examples/dist examples/aot && ngc -p examples",
    "example:run": "node examples/dist/examples/blink",
    "example:chip": "node examples/dist/examples/blink-chip",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urish/angular2-iot.git"
  },
  "keywords": [
    "angular2",
    "iot",
    "arduino",
    "robotics",
    "raspberry pi"
  ],
  "author": "Uri Shaked",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/urish/angular2-iot/issues"
  },
  "homepage": "https://github.com/urish/angular2-iot#readme",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/compiler-cli": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-server": "~2.2.0",
    "@types/johnny-five": "0.0.31",
    "@types/node": "^7.0.5",
    "angular2-universal": "2.1.0-rc.1",
    "angular2-universal-polyfills": "2.1.0-rc.1",
    "es6-shim": "^0.35.3",
    "johnny-five": "^0.10.6",
    "preboot": "^4.5.2",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.6.0",
    "rxjs": "5.0.0-beta.12",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6",
    "zone.js": "~0.6.26"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0",
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/platform-server": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "johnny-five": "^0.10.0"
  }
}
